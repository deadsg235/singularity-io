{
    "version": 2,
    "builds": [
        {
            "src": "/api/main.py",
            "use": "@vercel/python"
        },
        {
            "src": "/web/singularity-frontend/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/api/main.py"
        },
        {
            "src": "/(.*)",
            "dest": "/web/singularity-frontend/$1"
        }
    ]
}
