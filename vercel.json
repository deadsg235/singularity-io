{
  "buildCommand": "echo 'Python only build'",
  "installCommand": "pip install -r api/requirements.txt",
  "functions": {
    "api/index.py": {
      "runtime": "python3.12"
    }
  },
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.py"
    },
    {
      "src": "/(.*)",
      "dest": "/web/singularity-frontend/$1"
    }
  ]
}